<widget id="ch.zhaw.iap.family_persoenlichkeitsdiagnostik" ios_CFBundleIdentifier="ch.zhaw.iap.family_persoenlichkeitsdiagnostik" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
